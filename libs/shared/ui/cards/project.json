{
  "name": "card",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ui/cards/src",
  "prefix": "angular-monorepo",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/ui/cards/ng-package.json",
        "tailwindConfig": "libs/shared/ui/cards/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/ui/cards/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/ui/cards/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/ui/cards/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
